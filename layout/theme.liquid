<!DOCTYPE html>
<html lang="{{ request.locale.iso_code }}">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>
    {%- if page_title -%}
      {{ page_title }}
      {%- if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif -%}
    {%- else -%}
      {{ shop.name }}
    {%- endif -%}
  </title>

  {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}

  {% render 'meta-tags' %}

  <link rel="canonical" href="{{ canonical_url }}">

  {%- if settings.favicon != blank -%}
    <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
  {%- endif -%}

  {% comment %} Preconnect to Google Fonts {% endcomment %}
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

  {% comment %} Custom Fonts {% endcomment %}
  {% render 'css-fonts' %}

  {% comment %} CSS Stylesheets {% endcomment %}
  {{ 'normalize.css' | asset_url | stylesheet_tag }}
  {{ 'components.css' | asset_url | stylesheet_tag }}
  {{ 'sdhealth.css' | asset_url | stylesheet_tag }}

  {% comment %} Google Fonts - Saira {% endcomment %}
  <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
  <script>WebFont.load({ google: { families: ["Saira:regular,500,700"] } });</script>

  {% comment %} Webflow touch detection {% endcomment %}
  <script>
    !function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);
  </script>

  {{ content_for_header }}

  {% comment %} Theme CSS Variables Override {% endcomment %}
  <style>
    :root {
      --color-primary: {{ settings.color_primary }};
      --color-secondary: {{ settings.color_secondary }};
      --color-background: {{ settings.color_background }};
      --color-text: {{ settings.color_text }};
    }
  </style>
</head>
<body class="template-{{ template.name | handle }}{% if template.suffix %} template-{{ template.name }}-{{ template.suffix | handle }}{% endif %}">
  <a class="skip-to-content-link visually-hidden" href="#MainContent">
    {{ 'accessibility.skip_to_content' | t }}
  </a>

  <div class="page_wrap">
    {% comment %} Fixed UI elements container {% endcomment %}
    <div class="u-position-fixed">
      <div class="w-embed">
        <style>
          * {
            --_typography---font--primary-family: 'Saira', 'HarmonyOS Sans TC', sans-serif !important;
          }
          ::before, ::after {
            box-sizing: border-box;
          }
          .w-embed:before, .w-embed:after,
          .w-richtext:before, .w-richtext:after {
            content: unset;
          }
          html {
            background-color: var(--_theme---background);
          }
          button {
            background-color: unset;
            padding: unset;
            text-align: inherit;
          }
          button:not(:disabled) {
            cursor: pointer;
          }
          video {
            width: 100%;
            object-fit: cover;
          }
          .wf-empty {
            padding: 0;
          }
          svg {
            max-width: 100%;
          }
          @media (prefers-color-scheme: light) {
            option { color: black; }
          }
          img::selection {
            background: transparent;
          }
          body {
            font-smoothing: antialiased;
            -webkit-font-smoothing: antialiased;
          }
          a:not([class]) {
            text-decoration: underline;
          }
          h1,h2,h3,h4,h5,h6,p,blockquote,label {
            font-family: inherit;
            font-size: inherit;
            font-weight: inherit;
            line-height: inherit;
            letter-spacing: inherit;
            text-transform: inherit;
            text-wrap: inherit;
            margin-top: 0;
            margin-bottom: 0;
          }
          ::selection {
            background-color: var(--_theme---selection--background);
            color: var(--_theme---selection--text);
          }
        </style>
      </div>
    </div>

    {% comment %} Header Section {% endcomment %}
    {% section 'header' %}

    {% comment %} Main Content {% endcomment %}
    <main id="MainContent" class="page_main" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% comment %} Footer Section {% endcomment %}
    {% section 'footer' %}
  </div>

  {% comment %} JavaScript {% endcomment %}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site={{ shop.permanent_domain | md5 }}" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  {{ 'sdhealth.js' | asset_url | script_tag }}
  <script src="https://cdn.prod.website-files.com/gsap/3.14.2/gsap.min.js"></script>

  {% comment %} Nav scroll animation {% endcomment %}
  <script>
    (function() {
      const navLogo = document.querySelector(".nav_logo");
      if (!navLogo) return;
      let scrolled = false;
      const threshold = 50;
      function checkScroll() {
        const shouldBeScrolled = window.scrollY > threshold;
        if (shouldBeScrolled !== scrolled) {
          scrolled = shouldBeScrolled;
          navLogo.classList.toggle("is-scrolled", scrolled);
        }
      }
      window.addEventListener("scroll", checkScroll, { passive: true });
      checkScroll();
    })();
  </script>

  {% comment %} Dynamic year script {% endcomment %}
  <script>
    document.addEventListener("DOMContentLoaded", function () {
      document.querySelectorAll("[data-dynamic-year]").forEach(function (el) {
        el.textContent = new Date().getFullYear();
      });
    });
  </script>

  {% comment %} Vimeo Video Button {% endcomment %}
  <script>
    (function() {
      var vimeo = document.querySelectorAll(".vimeo");
      for (var i = 0; i < vimeo.length; i++) {
        var element = vimeo[i];
        var videoId = element.dataset.embed;
        if (element.dataset.thumbnail) {
          var image = new Image();
          image.src = element.dataset.thumbnail;
          image.loading = "lazy";
          element.appendChild(image);
        } else {
          (function(el, id) {
            fetch("https://vimeo.com/api/v2/video/" + id + ".json")
              .then(response => response.json())
              .then(data => {
                var image = new Image();
                image.src = data[0].thumbnail_large;
                image.loading = "lazy";
                el.appendChild(image);
              })
              .catch(err => console.log("Vimeo thumbnail error:", err));
          })(element, videoId);
        }
        element.addEventListener("click", function() {
          var iframe = document.createElement("iframe");
          iframe.setAttribute("frameborder", "0");
          iframe.setAttribute("allowfullscreen", "");
          iframe.setAttribute("allow", "autoplay; fullscreen; picture-in-picture");
          iframe.setAttribute("src", "https://player.vimeo.com/video/" + this.dataset.embed + "?autoplay=1&title=0&byline=0&portrait=0");
          this.innerHTML = "";
          this.appendChild(iframe);
        });
      }
    })();
  </script>
</body>
</html>
